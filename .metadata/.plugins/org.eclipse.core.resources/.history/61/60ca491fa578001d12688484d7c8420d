import javax.swing.*;

public class Test {

    public static void main(String[] args) {
        JFrame f=new JFrame("Please selsect meat.");
        JButton beef=new JButton("Beef");
        JButton pork=new JButton("Pork");
        JButton chick=new JButton("Chicken");
        JButton fish=new JButton("Fish");
        JButton shripm=new JButton("Shrimp");
        JButton sqiud=new JButton("Squid");
        JButton sea=new JButton("Seafood");
        
        beef.setBounds(150,30,130,30);
        f.add(beef);
        
        pork.setBounds(150,80,130,30);
        f.add(pork);
        
        chick.setBounds(150,130,130,30);
        f.add(chick);
        
        fish.setBounds(150,180,130,30);
        f.add(fish);
        
        shripm.setBounds(150,230,130,30);
        f.add(shripm);
        
        sqiud.setBounds(150,280,130,30);
        f.add(sqiud);
        
        sea.setBounds(150,330,130,30);
        f.add(sea);
        f.setSize(450,450);
        f.setLayout(null);
        f.setVisible(true);

    }

}